

	<!-- Contact -->
	<div id="contact" class="section md-padding">
		<!-- Container -->
		<div class="container">
			<!-- Row -->
			<div class="row">
				<!-- Section-header -->
				<div class="section-header text-center">
					<h2 class="title">Get in touch</h2>
				</div>
				<!-- /Section-header -->
				<!-- contact -->
				<div class="col-sm-4">
					<div class="contact">
						<a href="https://www.instagram.com/cardinalrecordings/?hl=en" target="_blank"><i class="fa fa-instagram"></i></a>
						<h3>Instagram</h3>
						<p>Follow us on Instagram</p>
					</div>
				</div>
				<!-- /contact -->
				<!-- contact -->
				<div class="col-sm-4">
					<div class="contact">
						<i class="fa fa-envelope"></i>
						<h3>Email</h3>
						<p>CardinalRecBooking@gmail.com</p>
					</div>
				</div>
				<!-- /contact -->
				<!-- contact -->
				<div class="col-sm-4">
					<div class="contact">
						<a href="https://www.facebook.com/cardinalrecordings/" target="_blank"><i class="fa fa-facebook-square"></i></a>
						<h3>facebook</h3>
						<p>Like us on Facebook</p>
